<%= form_for([@job, @interview]) do |f| %>
  <div class="form-group">
    <%= f.label :date_interviewed %><br>
    <%= f.date_field :date_interviewed, class: "form-control" %>
  </div>
    <div class="form-group">
  <div class="radio-inline">
  <%= f.radio_button :kind, 'Phone' %><b>Phone</b>
  </div>
  <div class="radio-inline">
    <%= f.radio_button :kind, 'In Person' %><b>In Person</b>
  </div>
  <div class="radio-inline">
    <%= f.radio_button :kind, 'Video Chat' %><b>Video</b>
  </div>
  </end> 
  <div class="form-group">
    <%= f.label :notes %><br>
    <%= f.text_area :notes, class: "form-control" %>
  </div>
</div>  
<div class="col-lg-12">      
  <div class="actions">
      <%= f.submit "Submit", class: "btn btn-large btn-primary" %>    
  </div> 
<% end %>